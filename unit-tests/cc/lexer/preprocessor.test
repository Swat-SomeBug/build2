# Test preprocessor lines.
#

: normal
:
$* <<EOI
#pragma message("abc")
EOI

: multiline
:
$* <<EOI
#pragma message \
( \
"abc" \
)
EOI

: comment
:
$* <<EOI
#pragma foo /*
bar
baz
*/
#pragma foo // bar baz
EOI

: line
:
$* <<EOI
# 1 "test.cxx" 2
#line 8 "z:\\tmp\\test.hxx"
EOI

: nested
:
$* <<EOI >>EOO
#define FOO(x) #y
;
EOI
';'
EOO
