# Test concatenated expansion.

.include ../common.testscript

: typed-exceptions
:
$* <<EOI >>/EOO
d = [dir_path] dir
s = [string] foo

print $d/{bar}
print $s{bar}
print $d/$s{bar}
print file{x$s}
print $d/{x$s}
print prj%{x$s}
EOI
dir/bar
foo{bar}
dir/foo{bar}
file{xfoo}
dir/xfoo
prj%xfoo
EOO
